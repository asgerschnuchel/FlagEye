<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"


    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:onClick="alpha"
    android:overScrollMode="never"
    android:scrollbars="none">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        tools:context="com.example.asger.flageye.HotelActivity">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Velkommen til Flageye V0.1 Alpha" />

        <ImageButton
            android:id="@+id/FoxtrotHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/CharlieHotel"
            android:layout_toEndOf="@+id/BravoHotel"
            android:layout_toRightOf="@+id/BravoHotel"
            android:onClick="foxtrot"
            app:srcCompat="@drawable/foxtrot" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView"
            android:layout_marginTop="44dp"
            android:text="    Vælg det flag som er øverst på standeren"
            android:textColor="@android:color/background_dark"
            android:textSize="18sp" />

        <ImageButton
            android:id="@+id/IndiaHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/FoxtrotHotel"
            android:layout_toEndOf="@+id/EchoHotel"
            android:layout_toRightOf="@+id/EchoHotel"
            app:srcCompat="@drawable/india" />

        <ImageButton
            android:id="@+id/CharlieHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignTop="@+id/BravoHotel"
            android:onClick="charlie"
            app:srcCompat="@drawable/charlie" />

        <ImageButton
            android:id="@+id/BravoHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="bravo"
            app:srcCompat="@drawable/bravo"
            android:layout_alignTop="@+id/AlphaHotel"
            android:layout_toLeftOf="@+id/CharlieHotel"
            android:layout_toStartOf="@+id/CharlieHotel" />

        <ImageButton
            android:id="@+id/EchoHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/BravoHotel"
            android:layout_alignStart="@+id/BravoHotel"
            android:layout_below="@+id/CharlieHotel"
            android:onClick="echo"
            app:srcCompat="@drawable/echo" />

        <ImageButton
            android:id="@+id/HotelHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/EchoHotel"
            android:layout_alignStart="@+id/EchoHotel"
            android:layout_below="@+id/FoxtrotHotel"
            app:srcCompat="@drawable/hotel" />

        <ImageButton
            android:id="@+id/DeltaHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/AlphaHotel"
            android:onClick="Delta"
            app:srcCompat="@drawable/delta" />

        <ImageButton
            android:id="@+id/AlphaHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="47dp"
            android:onClick="alpha"
            app:srcCompat="@drawable/alpha"
            android:layout_below="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/LimaHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/IndiaHotel"
            app:srcCompat="@drawable/lima" />

        <ImageButton
            android:id="@+id/OscarHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/LimaHotel"
            app:srcCompat="@drawable/oscar" />

        <ImageButton
            android:id="@+id/NovemberHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/KiloHotel"
            android:layout_alignStart="@+id/KiloHotel"
            android:layout_below="@+id/LimaHotel"
            app:srcCompat="@drawable/november" />

        <ImageButton
            android:id="@+id/JulietHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/GolfHotel"
            android:layout_alignStart="@+id/GolfHotel"
            android:layout_below="@+id/HotelHotel"
            app:srcCompat="@drawable/juliet" />

        <ImageButton
            android:id="@+id/MikeHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/JulietHotel"
            android:layout_alignStart="@+id/JulietHotel"
            android:layout_below="@+id/JulietHotel"
            app:srcCompat="@drawable/mike" />

        <ImageButton
            android:id="@+id/GolfHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/golf"
            android:layout_below="@+id/DeltaHotel"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/SierraHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PapaHotel"
            app:srcCompat="@drawable/sierra" />


        <ImageButton
            android:id="@+id/KiloHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/HotelHotel"
            android:layout_alignStart="@+id/HotelHotel"
            android:layout_below="@+id/IndiaHotel"
            app:srcCompat="@drawable/kilo" />

        <ImageButton
            android:id="@+id/RomeoHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/OscarHotel"
            android:layout_toEndOf="@+id/NovemberHotel"
            android:layout_toRightOf="@+id/NovemberHotel"
            app:srcCompat="@drawable/romeo" />

        <ImageButton
            android:id="@+id/PapaHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/NovemberHotel"
            app:srcCompat="@drawable/papa" />

        <ImageButton
            android:id="@+id/VictorHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/SierraHotel"
            app:srcCompat="@drawable/victor" />

        <ImageButton
            android:id="@+id/UniformHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/RomeoHotel"
            app:srcCompat="@drawable/uniform" />

        <ImageButton
            android:id="@+id/QuebeckHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/NovemberHotel"
            android:layout_alignStart="@+id/NovemberHotel"
            android:layout_below="@+id/OscarHotel"
            app:srcCompat="@drawable/quebec" />

        <ImageButton
            android:id="@+id/Tangomain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/QuebeckHotel"
            android:layout_alignStart="@+id/QuebeckHotel"
            android:layout_below="@+id/PapaHotel"
            app:srcCompat="@drawable/tango" />

        <ImageButton
            android:id="@+id/WhiskeyHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/Tangomain"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/Tangomain"
            app:srcCompat="@drawable/whisky" />

        <ImageButton
            android:id="@+id/XrayHotel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/UniformHotel"
            android:layout_toEndOf="@+id/WhiskeyHotel"
            android:layout_toRightOf="@+id/WhiskeyHotel"
            app:srcCompat="@drawable/xray" />

        <ImageButton
            android:id="@+id/about"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@android:color/background_light"
            android:onClick="about"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/info" />

    </RelativeLayout>
</ScrollView>
