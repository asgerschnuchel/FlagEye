<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_alignParentBottom="true"
    android:layout_height="match_parent"


    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:onClick="alpha"
    android:overScrollMode="never"
    android:scrollbars="none">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:gravity="center|bottom"
        tools:context="com.example.asger.flageye.KiloActivity">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Velkommen til Flageye V0.1 Alpha" />

        <ImageButton
            android:id="@+id/FoxtrotZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="foxtrot"
            app:srcCompat="@drawable/foxtrot"
            android:layout_below="@+id/CharlieZulu"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView"
            android:layout_marginTop="44dp"
            android:text="    Vælg det flag som er øverst på standeren"
            android:textColor="@android:color/background_dark"
            android:textSize="18sp" />

        <ImageButton
            android:id="@+id/IndiaZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/FoxtrotZulu"
            android:onClick="india"
            app:srcCompat="@drawable/india" />

        <ImageButton
            android:id="@+id/CharlieZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="charlie"
            app:srcCompat="@drawable/charlie"
            android:layout_alignTop="@+id/BravoZulu"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:id="@+id/BravoZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="bravo"
            app:srcCompat="@drawable/bravo"
            android:layout_alignTop="@+id/AlphaZulu"
            android:layout_toLeftOf="@+id/CharlieZulu"
            android:layout_toStartOf="@+id/CharlieZulu" />

        <ImageButton
            android:id="@+id/EchoZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="echo"
            app:srcCompat="@drawable/echo"
            android:layout_below="@+id/CharlieZulu"
            android:layout_toLeftOf="@+id/FoxtrotZulu"
            android:layout_toStartOf="@+id/FoxtrotZulu" />

        <ImageButton
            android:id="@+id/HotelZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="hotel"
            app:srcCompat="@drawable/hotel"
            android:layout_below="@+id/FoxtrotZulu"
            android:layout_alignLeft="@+id/EchoZulu"
            android:layout_alignStart="@+id/EchoZulu" />

        <ImageButton
            android:id="@+id/DeltaZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="delta"
            app:srcCompat="@drawable/delta"
            android:layout_below="@+id/AlphaZulu"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/AlphaZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="alpha"
            app:srcCompat="@drawable/alpha"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/textView2"
            android:layout_alignParentLeft="true"
            android:layout_toLeftOf="@+id/BravoZulu"
            android:layout_toStartOf="@+id/BravoZulu" />

        <ImageButton
            android:id="@+id/LimaZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/IndiaZulu"
            android:onClick="lima"
            app:srcCompat="@drawable/lima" />

        <ImageButton
            android:id="@+id/OscarZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="oscar"
            android:layout_below="@+id/LimaZulu"
            app:srcCompat="@drawable/oscar" />

        <ImageButton
            android:id="@+id/NovemberZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/november"
            android:onClick="november"
            android:layout_below="@+id/LimaZulu"
            android:layout_alignLeft="@+id/KiloZulu"
            android:layout_alignStart="@+id/KiloZulu" />

        <ImageButton
            android:id="@+id/JulietZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="juliet"
            app:srcCompat="@drawable/juliet"
            android:layout_below="@+id/GolfZulu"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/MikeZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/JulietZulu"
            android:onClick="mike"
            app:srcCompat="@drawable/mike" />

        <ImageButton
            android:id="@+id/GolfZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="golf"
            app:srcCompat="@drawable/golf"
            android:layout_below="@+id/DeltaZulu"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/SierraZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PapaZulu"
            android:onClick="sierra"
            app:srcCompat="@drawable/sierra" />


        <ImageButton
            android:id="@+id/KiloZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/HotelZulu"
            android:layout_alignStart="@+id/HotelZulu"
            android:layout_below="@+id/IndiaZulu"
            android:onClick="kilo"
            app:srcCompat="@drawable/kilo" />

        <ImageButton
            android:id="@+id/RomeoZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/romeo"
            android:onClick="romeo"
            android:layout_below="@+id/OscarZulu"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:id="@+id/PapaZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/NovemberZulu"
            app:srcCompat="@drawable/papa" />

        <ImageButton
            android:id="@+id/VictorZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="victor"
            app:srcCompat="@drawable/victor"
            android:layout_below="@+id/SierraZulu"
            android:layout_toLeftOf="@+id/WhiskyZulu"
            android:layout_toStartOf="@+id/WhiskyZulu" />

        <ImageButton
            android:id="@+id/UniformZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/RomeoZulu"
            android:onClick="uniform"
            app:srcCompat="@drawable/uniform" />

        <ImageButton
            android:id="@+id/QuebeckZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/NovemberZulu"
            android:layout_alignStart="@+id/NovemberZulu"
            android:onClick="quebeck"
            android:layout_below="@+id/OscarZulu"
            app:srcCompat="@drawable/quebec" />

        <ImageButton
            android:id="@+id/Tangomain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/QuebeckZulu"
            android:layout_centerHorizontal="true"
            android:onClick="tango"
            app:srcCompat="@drawable/tango" />

        <ImageButton
            android:id="@+id/WhiskyZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="whisky"
            app:srcCompat="@drawable/whisky"
            android:layout_below="@+id/UniformZulu"
            android:layout_toLeftOf="@+id/UniformZulu"
            android:layout_toStartOf="@+id/UniformZulu" />


        <ImageButton
            android:id="@+id/about"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@android:color/background_light"
            android:onClick="about"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/info" />

        <ImageButton
            android:id="@+id/XrayZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/xray"
            android:layout_below="@+id/UniformZulu"
            android:layout_toRightOf="@+id/WhiskyZulu"
            android:layout_toEndOf="@+id/WhiskyZulu" />

        <ImageButton
            android:id="@+id/YankeeZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/VictorZulu"
            android:layout_alignStart="@+id/WhiskyZulu"
            android:layout_below="@+id/WhiskyZulu"
            app:srcCompat="@drawable/yankee" />

        <ImageButton
            android:id="@+id/ZuluZulu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/zulu"
            android:layout_below="@+id/VictorZulu"
            android:layout_alignLeft="@+id/Tangomain"
            android:layout_alignStart="@+id/Tangomain" />


    </RelativeLayout>
</ScrollView>
