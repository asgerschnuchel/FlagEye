<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_alignParentBottom="true"
    android:layout_height="match_parent"


    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:onClick="alpha"
    android:overScrollMode="never"
    android:scrollbars="none">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="0dp"
        android:paddingLeft="0dp"
        android:paddingRight="0dp"
        android:paddingTop="0dp"
        android:gravity="center|bottom"
        tools:context="com.example.asger.flageye.KiloActivity">

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:text="Velkommen til Flageye V0.1 Alpha" />

        <ImageButton
            android:id="@+id/FoxtrotGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="foxtrot"
            app:srcCompat="@drawable/foxtrot"
            android:layout_below="@+id/CharlieGolf"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView"
            android:layout_marginTop="44dp"
            android:text="    Vælg det flag som er øverst på standeren"
            android:textColor="@android:color/background_dark"
            android:textSize="18sp" />

        <ImageButton
            android:id="@+id/IndiaGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/FoxtrotGolf"
            android:onClick="india"
            app:srcCompat="@drawable/india" />

        <ImageButton
            android:id="@+id/CharlieGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="charlie"
            app:srcCompat="@drawable/charlie"
            android:layout_alignTop="@+id/BravoGolf"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:id="@+id/BravoGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="bravo"
            app:srcCompat="@drawable/bravo"
            android:layout_alignTop="@+id/AlphaGolf"
            android:layout_toLeftOf="@+id/CharlieGolf"
            android:layout_toStartOf="@+id/CharlieGolf" />

        <ImageButton
            android:id="@+id/EchoGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="echo"
            app:srcCompat="@drawable/echo"
            android:layout_below="@+id/CharlieGolf"
            android:layout_toLeftOf="@+id/FoxtrotGolf"
            android:layout_toStartOf="@+id/FoxtrotGolf" />

        <ImageButton
            android:id="@+id/HotelGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="hotel"
            app:srcCompat="@drawable/hotel"
            android:layout_below="@+id/FoxtrotGolf"
            android:layout_alignLeft="@+id/EchoGolf"
            android:layout_alignStart="@+id/EchoGolf" />

        <ImageButton
            android:id="@+id/DeltaGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="delta"
            app:srcCompat="@drawable/delta"
            android:layout_below="@+id/AlphaGolf"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/AlphaGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="alpha"
            app:srcCompat="@drawable/alpha"
            android:layout_marginTop="10dp"
            android:layout_below="@+id/textView2"
            android:layout_toLeftOf="@+id/BravoGolf"
            android:layout_toStartOf="@+id/BravoGolf" />

        <ImageButton
            android:id="@+id/LimaGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/IndiaGolf"
            android:onClick="lima"
            app:srcCompat="@drawable/lima" />

        <ImageButton
            android:id="@+id/OscarGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:onClick="oscar"
            android:layout_below="@+id/LimaGolf"
            app:srcCompat="@drawable/oscar" />

        <ImageButton
            android:id="@+id/NovemberGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/november"
            android:onClick="november"
            android:layout_below="@+id/LimaGolf"
            android:layout_alignLeft="@+id/KiloGolf"
            android:layout_alignStart="@+id/KiloGolf" />

        <ImageButton
            android:id="@+id/JulietGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="juliet"
            app:srcCompat="@drawable/juliet"
            android:layout_below="@+id/GolfGolf"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/MikeGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/JulietGolf"
            android:onClick="mike"
            app:srcCompat="@drawable/mike" />

        <ImageButton
            android:id="@+id/GolfGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="golf"
            app:srcCompat="@drawable/golf"
            android:layout_below="@+id/DeltaGolf"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:id="@+id/SierraGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/PapaGolf"
            android:onClick="sierra"
            app:srcCompat="@drawable/sierra" />


        <ImageButton
            android:id="@+id/KiloGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/HotelGolf"
            android:layout_alignStart="@+id/HotelGolf"
            android:layout_below="@+id/IndiaGolf"
            android:onClick="kilo"
            app:srcCompat="@drawable/kilo" />

        <ImageButton
            android:id="@+id/RomeoGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/romeo"
            android:onClick="romeo"
            android:layout_below="@+id/OscarGolf"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />

        <ImageButton
            android:id="@+id/PapaGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/NovemberGolf"
            app:srcCompat="@drawable/papa" />

        <ImageButton
            android:id="@+id/VictorGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="victor"
            app:srcCompat="@drawable/victor"
            android:layout_below="@+id/SierraGolf"
            android:layout_toLeftOf="@+id/WhiskyGolf"
            android:layout_toStartOf="@+id/WhiskyGolf" />

        <ImageButton
            android:id="@+id/UniformGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/RomeoGolf"
            android:onClick="uniform"
            app:srcCompat="@drawable/uniform" />

        <ImageButton
            android:id="@+id/QuebeckGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/NovemberGolf"
            android:layout_alignStart="@+id/NovemberGolf"
            android:onClick="quebeck"
            android:layout_below="@+id/OscarGolf"
            app:srcCompat="@drawable/quebec" />

        <ImageButton
            android:id="@+id/Tangomain"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/QuebeckGolf"
            android:layout_centerHorizontal="true"
            android:onClick="tango"
            app:srcCompat="@drawable/tango" />

        <ImageButton
            android:id="@+id/WhiskyGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="whisky"
            app:srcCompat="@drawable/whisky"
            android:layout_below="@+id/UniformGolf"
            android:layout_toLeftOf="@+id/UniformGolf"
            android:layout_toStartOf="@+id/UniformGolf" />


        <ImageButton
            android:id="@+id/about"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:background="@android:color/background_light"
            android:onClick="about"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/info" />

        <ImageButton
            android:id="@+id/XrayGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/xray"
            android:layout_below="@+id/UniformGolf"
            android:layout_toRightOf="@+id/WhiskyGolf"
            android:layout_toEndOf="@+id/WhiskyGolf" />

        <ImageButton
            android:id="@+id/YankeeGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/VictorGolf"
            android:layout_alignStart="@+id/WhiskyGolf"
            android:layout_below="@+id/WhiskyGolf"
            app:srcCompat="@drawable/yankee" />

        <ImageButton
            android:id="@+id/ZuluGolf"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/zulu"
            android:layout_below="@+id/VictorGolf"
            android:layout_alignLeft="@+id/Tangomain"
            android:layout_alignStart="@+id/Tangomain" />


    </RelativeLayout>
</ScrollView>
